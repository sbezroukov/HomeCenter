@model QuizApp.Models.TestTreeNode

@{
    ViewData["Title"] = "Доступные темы";
}

<h2>Доступные темы</h2>

@if (Model.Topics.Count == 0 && Model.Children.Count == 0)
{
    <p>Пока нет доступных тем. Обратитесь к администратору.</p>
}
else
{
    await Html.RenderPartialAsync("_TestTreeLevel", (Model, 0));
}
